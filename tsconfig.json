{  
  "compilerOptions": {  
    "target": "es6",  
    "module": "commonjs",  
    "strict": true,  
    "esModuleInterop": true,  
    "skipLibCheck": true,  
    "forceConsistentCasingInFileNames": true,  
    "jsx": "preserve"  ,
    "types": ["cypress"]
  },  
  "include": [  
    "cypress/**/*.ts"  
  ],  
  "exclude": [  
    "node_modules"  
  ]  
}  